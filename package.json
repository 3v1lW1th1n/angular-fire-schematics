{
  "name": "angular-fire-schematic",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "git checkout HEAD -- sandbox && git clean -f -d sandbox",
    "link:schematic": "yarn link && cd sandbox && yarn link \"angular-fire-schematic\"",
    "sandbox:add": "cd sandbox && ng g angular-fire-schematic:add",
    "test": "yarn clean && yarn sandbox:add && yarn test:sandbox",
    "test:unit": "yarn build && jasmine src/**/*_spec.js",
    "test:sandbox": "cd sandbox && yarn lint && yarn test --watch=false && yarn build"
  },
  "keywords": [
    "schematics"
  ],
  "author": "Brian Love",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "engines": {
    "node": ">=8.11.0"
  },
  "dependencies": {
    "@angular-devkit/core": "^7.1.3",
    "@angular-devkit/schematics": "^7.1.3",
    "@angular/cdk": "^7.1.1",
    "@schematics/angular": "^7.1.3",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "jasmine": "^2.8.0",
    "tslint": "^5.11.0",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.11.3",
    "semantic-release": "^15.13.0"
  }
}
